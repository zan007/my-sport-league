{
    "name": "my-sport-league",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve  --proxy-config ./proxy.json",
        "server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "lint:css": "stylelint \"./src/**/*.scss\"",
        "format:html": "prettier --write src/app/**/*.html",
        "format:ts": "prettier --write ./**/*.ts",
        "format:scss": "stylelint --fix ./src/**/*.scss",
        "format:all": "npm run format:html && npm run format:scss && npm run format:ts",
        "iconfont": "node build-process/scripts/icon-font.loader.js",
        "e2e": "ng e2e"
    },
    "private": true,
